{
  "name": "footle-ui",
  "version": "0.0.0",
  "description": "Web-based UI for the Footle PHP debugger.",
  "main": "scripts/ui.js",
  "scripts": {
    "build:script": "webpack -d",
    "build:style": "node-sass --indented-syntax --source-map true style/sass/ui.sass build/style/css/ui.css",
    "build:release:script": "webpack -p --entry ./scripts/ui.js --output build/release/scripts/ui-bundle.js",
    "build:release:style": "node-sass --indented-syntax style/sass/ui.sass build/release/style/css/ui.css"
  },
  "author": "Progga",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "bower": "^1.8.4",
    "node-sass": "^4.9.3",
    "standard": "^11.0.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {}
}
